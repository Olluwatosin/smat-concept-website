# Django Configuration
# Copy this file to .env and update with your actual values

# SECURITY WARNING: Generate a unique secret key for production!
# You can generate one using: python -c 'from django.core.management.utils import get_random_secret_key; print(get_random_secret_key())'
DJANGO_SECRET_KEY=your-secret-key-here

# SECURITY WARNING: Set to False in production!
DJANGO_DEBUG=True

# Comma-separated list of allowed hosts (e.g., localhost,127.0.0.1,yourdomain.com)
DJANGO_ALLOWED_HOSTS=localhost,127.0.0.1
